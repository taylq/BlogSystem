<% if @feed_items.any? %>
  <div class="custom_a">
    <div class="flickr_pagination">
        <ol class="blogs">
          <%= render @feed_items %>
        </ol>
      <div class="page_info">
        <%= page_entries_info @feed_items %>
      </div>
      <%= will_paginate @feed_items, container: false %>
    </div>
  </div>
<% end %>
